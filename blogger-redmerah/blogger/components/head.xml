            <style>h1{text-align:center;font-weight:400;font-size:40px;padding:32px 16px}main{display:flex;flex-direction:column;align-items:center;gap:8px;width:700px;max-width:100%;padding:0 8px}main details{background-color:var(--color-background-hover);width:100%;border:1px solid var(--color-background-outline);border-radius:4px;transition:all .25s}main details[open]&gt;summary{border-bottom:1px solid var(--color-background-outline);border-radius:3px 3px 0 0}main details[open]&gt;summary&gt;.icon{transform:rotate(180deg)}main summary{display:flex;list-style:none;align-items:center;justify-content:space-between;padding:16px;border-radius:3px;transition:all .25s}@media (hover:hover){main summary:hover{background-color:var(--color-background-outline)}}main summary:active{background-color:var(--color-background-outline);padding:16px 24px}main .icon{transition:all .25s}main div{padding:8px;display:flex;flex-direction:column;gap:8px}main a{color:var(--color-on-background);padding:0 8px;transition:all .25s}@media (hover:hover){main a:hover{text-decoration:underline}}main a:active{text-decoration:underline;padding:0 16px}.ap-month{background-color:var(--color-background-outline)}.ap-month[open]&gt;summary{border-bottom:1px solid var(--color-outline)}@media (hover:hover){.ap-month summary:hover{background-color:var(--color-background)}}.ap-month summary:active{background-color:var(--color-background)}.ap-year[open]&gt;summary{padding:16px 24px}.ap-year[open]&gt;summary:active{padding:16px 32px}#_archives{max-width:100%;width:100%}#_archives&gt;div{max-width:100%;width:100%;display:flex;flex-direction:column;align-items:center}</style>

            <!-- Contact page -->
            <b:elseif cond='data:blog.url == path(data:blog.homepageUrl, "/p/contact.html")'/>
            <style>.cp-form{max-width:100%;width:100%;padding:0 16px 64px 16px;display:flex;flex-direction:column;align-items:center}.cp-form h1{text-align:center;font-weight:400;font-size:40px;padding:32px 16px}.cp-message{width:550px;max-width:100%}.cp-message textarea{border:1px solid var(--color-outline);border-bottom-width:4px;border-radius:4px;width:100%;padding:8px;font-size:16px;resize:none;transition:all .25s;margin-bottom:16px}.cp-message textarea::placeholder{color:var(--color-on-background)}@media (hover:hover){.cp-message textarea:hover{background-color:var(--color-background-hover)}}.cp-message textarea:focus{background-color:var(--color-background-hover);border-bottom-color:var(--color-primary)}.cp-message button{border-radius:4px;border:1px solid var(--color-outline);color:var(--color-on-background);display:block;margin-bottom:8px;padding:12px 16px;transition:all .25s}@media (hover:hover){.cp-message button:hover{background-color:var(--color-background-hover)}}.cp-message button:active{background-color:var(--color-background-hover)}.cp-message button:active&gt;*{scale:0.85}.cp-message div{display:flex;align-items:center;gap:8px;font-size:1rem;transition:all .25s}.cp-profile{display:grid;grid-template-columns:repeat(2,1fr);gap:16px;width:550px;max-width:100%;margin-bottom:16px}@media (max-width:450px){.cp-profile{grid-template-columns:1fr}}.cp-profile input{border:1px solid var(--color-outline);border-bottom-width:4px;border-radius:4px;width:100%;padding:8px;font-size:16px;resize:none;transition:all .25s}.cp-profile input::placeholder{color:var(--color-on-background)}@media (hover:hover){.cp-profile input:hover{background-color:var(--color-background-hover)}}.cp-profile input:focus{background-color:var(--color-background-hover);border-bottom-color:var(--color-primary)}#_contact{max-width:100%;width:100%}#_contact&gt;div{max-width:100%;width:100%}</style>

            <!-- Labels page -->
            <b:elseif cond='data:blog.url == path(data:blog.homepageUrl, "/p/labels.html")'/>
            <style>h1{text-align:center;font-weight:400;font-size:40px;padding:32px 16px}.lp-list{display:flex;flex-wrap:wrap;align-items:center;justify-content:center;gap:8px}.lp-list a{display:block;color:var(--color-on-background);padding:8px 16px 8px 10px;font-family:&quot;DM Mono&quot;,monospace;border-radius:4px;border:1px solid var(--color-outline);transition:all .25s}@media (hover:hover){.lp-list a:hover{background-color:var(--color-background-hover)}}.lp-list a:active{background-color:var(--color-background-hover)}.lp-list a:active&gt;*{scale:0.85}.lp-list div{display:flex;align-items:center;gap:16px;transition:all .25s}.lp-list p::before{content:&quot;[ &quot;}.lp-list p::after{content:&quot; ]&quot;}#_labels{width:100%;max-width:100%}#_labels&gt;div{width:100%;max-width:100%;padding:0 16px}</style>

        </b:if>

        <!-- Error page -->
        <b:else/>
        <style>main{width:700px;max-width:100%;display:flex;flex-direction:column;justify-content:center;padding:64px 32px 32px 32px}main h1{color:#d01717;font-size:108px;margin-bottom:24px}main h2{font-size:32px;margin-bottom:8px}main p{font-size:20px;margin-bottom:32px}main div{display:flex;align-items:center;gap:8px;transition:all .25s}main a{border-radius:4px;border:1px solid var(--color-outline);color:var(--color-on-background);display:block;padding:12px 16px;transition:all .25s}@media (hover:hover){main a:hover{background-color:var(--color-background-hover)}}main a:active{background-color:var(--color-background-hover)}main a:active&gt;*{scale:0.85}</style>

    </b:if>
    <!-- Styles end -->

</head>